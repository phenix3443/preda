{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "compile event contract on windows",
      "type": "cppvsdbg",
      "request": "launch",
      "windows": {
        "program": "${workspaceFolder}/oxd_preda/bin/bin_debug/chsimu.exe"
      },
      "osx": {
        "program": "${workspaceFolder}/oxd_preda/bin/bin_debug/chsimu"
      },
      "args": [
        "-log",
        "${workspaceFolder}/oxd_preda/transpiler/testcase/event.prd",
        "-stdout"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/oxd_preda/bin/bin_debug/",
      "environment": [],
      "console": "internalConsole"
    },
    {
      "name": "debug event contract on windows",
      "type": "cppvsdbg",
      "request": "launch",
      "windows": {
        "program": "${workspaceFolder}/oxd_preda/bin/bin_debug/chsimu.exe"
      },
      "osx": {
        "program": "${workspaceFolder}/oxd_preda/bin/bin_debug/chsimu"
      },
      "args": [
        "${workspaceFolder}/oxd_preda/transpiler/testcase/event.prdts",
        "-count:10",
        "-stdout"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/oxd_preda/bin/bin_debug/",
      "environment": [],
      "console": "internalConsole"
    },
    {
      "name": "debug encrypt contract on windows",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/oxd_preda/bin/bin_debug/chsimu.exe",
      "args": [
        "${workspaceFolder}/oxd_preda/transpiler/testcase/encrypt.prdts",
        "-count:4",
        "-stdout"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/oxd_preda/bin/bin_debug/",
      "environment": []
    },
    {
      "name": "debug encrypt contract on osx",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/oxd_preda/bin/bin_release/chsimu",
      "args": [
        "${workspaceFolder}/oxd_preda/transpiler/testcase/encrypt.prdts",
        "-count:4",
        "-stdout"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/oxd_preda/bin/bin_release/",
      "environment": [],
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/local/bin/gdb"
    }
  ]
}
